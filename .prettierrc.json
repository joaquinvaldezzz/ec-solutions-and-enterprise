{
  "printWidth": 100,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "@prettier/plugin-xml",
    "prettier-plugin-packagejson",
    "prettier-plugin-sh",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^types$",
    "^@/types/(.*)$",
    "^@/config/(.*)$",
    "^@/lib/(.*)$",
    "^@/hooks/(.*)$",
    "^@/components/ui/(.*)$",
    "^@/components/(.*)$",
    "^@/registry/(.*)$",
    "^@/styles/(.*)$",
    "^@/app/(.*)$",
    "^@/pages/(.*)$",
    "",
    "^[./]",
    "",
    "^@/public/(.*)$"
  ],
  "tailwindStylesheet": "src/styles/main.css",
  "tailwindAttributes": ["tw", "classNames"],
  "tailwindFunctions": ["cva"]
}
